# Set default line styles and colors
set style line 1 linetype 1 linewidth 2 #purple line
set style line 2 linetype 2 linewidth 2 #green line
set style line 3 linetype 3 linewidth 2 #blue line

# Set common axis options
set xlabel "X-axis Label"
set ylabel "Y-axis Label"
set grid

# Set font for titles and labels
set font "arial,10"  # Example font and size, adjust as desired
